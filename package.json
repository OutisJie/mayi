{
  "name": "mayi",
  "version": "1.0.0",
  "description": "outisjie",
  "main": "index.js",
  "author": "outisjie",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --progress --config webpack/webpack.prod.config.js",
    "start": "webpack-dev-server --progress --config webpack/webpack.dev.config.js",
    "lint-fix": "eslint './src/**/*.{ts,tsx}' --fix --ignore-pattern .gitignore",
    "eslint-fix": "eslint './src/**/*.{ts,tsx}' --fix",
    "precommit-msg": "echo Pre-commit checking...please wait && exit 0",
    "server": "nodemon server.js"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^17.0.0-rc.2",
    "@types/lodash": "^4.14.149",
    "@types/moment": "^2.13.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^5.1.0",
    "ghooks": "^2.0.4",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.6",
    "react-hot-loader": "^4.12.19",
    "style-loader": "^1.1.3",
    "ts-import-plugin": "^1.6.5",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "url-loader": "^3.0.0",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.2",
    "@types/papaparse": "^5.2.4",
    "antd": "^4.0.2",
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "immer": "^6.0.5",
    "jschardet": "^2.2.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "papaparse": "^5.3.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": ""
    },
    "ghooks": {
      "pre-commit": "npm run precommit-msg && npm run lint",
      "commit-msg": "validate-commit-msg"
    }
  }
}
